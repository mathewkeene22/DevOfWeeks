<% @votes.each do |votes| %>
  <p><%= votes.nominee_id %></p>
<% end %>

<%= form_with model: @vote, class: 'foobar' do |f| %>
  <%= f.text_area :election_id, as: :hidden %>
  <%= f.select :nominee_id, User.all.collect {|u| [ u.email, u.id ] } %>
  <%= f.text_area :message, size: "60x12" %>
  <%= f.submit "Create" %>
<% end %>

